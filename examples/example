void func(int value){
  write(value);
  return ;
}

int main(){
  string str = "this is a string";
  int var = 3;
  int arr[3];

  arr[0] = 10;
  
  if(arr[0] < 1000){
    var = 5;
  }

  if(var >= 3){
    str = "this is another string";
  }
  else{
    var = 1;
  }

  // this is a comment

  while(var){
    func(var);
    var = var - 1;
  }

  return 0;
}